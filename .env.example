# Private key for contract deployment (64 hex characters, with or without 0x prefix)
# Example format: 0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
PRIVATE_KEY=your_private_key_here

# Blockscout API key for contract verification
BLOCKSCOUT_API_KEY=your_blockscout_api_key_here

# Camp Network Client ID for Origin SDK
NEXT_PUBLIC_CAMP_CLIENT_ID=your_camp_client_id_here

# NFT.Storage API token for IPFS uploads
NFT_STORAGE_API_KEY=your_nft_storage_api_key_here

# Twitter/X Bearer Token for social sharing (optional)
NEXT_PUBLIC_X_BEARER_TOKEN=your_twitter_bearer_token_here

# WalletConnect Project ID for Web3Modal
NEXT_PUBLIC_WC_PROJECT_ID=your_walletconnect_project_id_here

# RPC URL for Basecamp testnet (optional - defaults to public RPC)
BASECAMP_RPC_URL=https://rpc.basecamp.t.raas.gelato.cloud/

# MongoDB Connection String
MONGODB_URI=your_mongodb_connection_string_here

